#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/khairulhaaziq/dev/js/vue/template-ui/node_modules/.pnpm/@morev+vue-transitions@2.3.6_vue@3.3.4/node_modules/@morev/vue-transitions/bin/node_modules:/Users/khairulhaaziq/dev/js/vue/template-ui/node_modules/.pnpm/@morev+vue-transitions@2.3.6_vue@3.3.4/node_modules/@morev/vue-transitions/node_modules:/Users/khairulhaaziq/dev/js/vue/template-ui/node_modules/.pnpm/@morev+vue-transitions@2.3.6_vue@3.3.4/node_modules/@morev/node_modules:/Users/khairulhaaziq/dev/js/vue/template-ui/node_modules/.pnpm/@morev+vue-transitions@2.3.6_vue@3.3.4/node_modules:/Users/khairulhaaziq/dev/js/vue/template-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/khairulhaaziq/dev/js/vue/template-ui/node_modules/.pnpm/@morev+vue-transitions@2.3.6_vue@3.3.4/node_modules/@morev/vue-transitions/bin/node_modules:/Users/khairulhaaziq/dev/js/vue/template-ui/node_modules/.pnpm/@morev+vue-transitions@2.3.6_vue@3.3.4/node_modules/@morev/vue-transitions/node_modules:/Users/khairulhaaziq/dev/js/vue/template-ui/node_modules/.pnpm/@morev+vue-transitions@2.3.6_vue@3.3.4/node_modules/@morev/node_modules:/Users/khairulhaaziq/dev/js/vue/template-ui/node_modules/.pnpm/@morev+vue-transitions@2.3.6_vue@3.3.4/node_modules:/Users/khairulhaaziq/dev/js/vue/template-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/switch.js" "$@"
else
  exec node  "$basedir/../../bin/switch.js" "$@"
fi
